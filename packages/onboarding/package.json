{
  "name": "@tripsam/onboarding",
  "version": "1.0.75",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./safari": {
      "types": "./dist/safari/index.d.ts",
      "import": "./dist/safari/index.js",
      "require": "./dist/safari/index.cjs"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "import": "./dist/common/index.js",
      "require": "./dist/common/index.cjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/safari/index.ts src/common/index.ts  --dts --format esm,cjs --out-dir dist --clean --splitting false",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.5.4",
    "rimraf": "^5.0.0",
    "zod": "^4.1.11"
  },
  "publishConfig": {
    "access": "public"
  }
}